<h1><%=@doctor.name%></h1><br>

  <%@doctor.appointments.each do |appointment|%>
    <%=appointment.appointment_datetime.strftime('%B %d, %Y at %H:%M')%><br>
    <%=link_to appointment.patient.name, patient_path(appointment.patient)%><br><br>
    <%end%>



  <ul>
  <% @doctor.patients.each do |patient|%>

  <li><%=link_to patient.name%></li><br>
  <%end%>
  </ul>
